{
  "name": "css-frameworks-ca",
  "version": "1.0.1",
  "description": "Test",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install",
    "test": "npm run test-e2e",
    "test-e2e": "cypress open",
    "test-unit": "jest",
    "format": "prettier -w src/**/*.js"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.25",
    "@babel/preset-env": "7.25",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "cypress": "^13.15.0",
    "eslint": "^9.13.0",
    "eslint-plugin-cypress": "^4.0.0",
    "eslint-plugin-jest": "^28.8.3",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "live-server": "^1.2.2",
    "prettier": "^3.3.3",
    "sass": "^1.80.3"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write .",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write ."
    ],
    "*.scss": [
      "prettier --write ."
    ]
  }
}
